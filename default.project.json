{
	"name": "PC Factory Tycoon",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"$path": "out/ServerScriptService"
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"$path": "out/ReplicatedStorage",
			"Events": {
				"$className": "Folder",
				"placeTileCheck": {
					"$className": "RemoteFunction"
				},
				"setPlayerPlot": {
					"$className": "RemoteEvent"
				},
				"setConveyerBeams": {
					"$className": "RemoteEvent"
				},
				"removeTile": {
					"$className": "RemoteEvent"
				},
				"conveyerContentUpdate": {
					"$className": "RemoteEvent"
				},
				"destroyConveyer": {
					"$className": "RemoteEvent"
				},
				"earnMoney": {
					"$className": "RemoteEvent"
				},
				"getTile": {
					"$className": "RemoteFunction"
				},
				"changeGeneratorRessource": {
					"$className": "RemoteEvent"
				},
				"changeCrafterOrAssemblerCraft": {
					"$className": "RemoteEvent"
				},
				"sendTileGrid": {
					"$className": "RemoteEvent"
				},
				"rotateTile": {
					"$className": "RemoteEvent"
				},
				"resetPlot": {
					"$className": "BindableEvent"
				},
				"playerQuests": {
					"$className": "RemoteEvent"
				},
				"resetQuests": {
					"$className": "BindableEvent"
				},
				"tileEntityProccessing": {
					"$className": "RemoteEvent"
				},
				"tileEntityUpdate": {
					"$className": "RemoteEvent"
				},
				"unlockedTileList": {
					"$className": "RemoteEvent"
				},
				"saveSettings" : {
					"$className": "RemoteEvent"
				},
				"loadSettings" : {
					"$className": "RemoteEvent"
				},
				"questCompleted" : {
					"$className": "RemoteEvent"
				}
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"$path": "out/StarterPlayerScripts"
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
